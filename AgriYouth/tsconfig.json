{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "esModuleInterop": true,               // Added to enable default imports
    "allowSyntheticDefaultImports": true   // Added to allow synthetic imports
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/Register.jsx"  ]
}
